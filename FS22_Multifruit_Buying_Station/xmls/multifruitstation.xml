		<fillType name="SEEDS" priceScale="0.75"/>			
		<fillType name="FERTILIZER" priceScale="0.75"/>
		<fillType name="LIQUIDFERTILIZER" priceScale="0.75"/>
		<fillType name="MANURE" priceScale="0.75"/>
		<fillType name="LIQUIDMANURE" priceScale="0.75"/>
