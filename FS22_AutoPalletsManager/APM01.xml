		<i3dMapping id="spawnPlace3End_01" node="0|3|11|0"/>
		<i3dMapping id="spawnPlace4Start_01" node="0|3|12"/>
		<i3dMapping id="spawnPlace4End_01" node="0|3|12|0"/>
		<i3dMapping id="spawnPlace5Start_01" node="0|3|13"/>
		<i3dMapping id="spawnPlace5End_01" node="0|3|13|0"/>
