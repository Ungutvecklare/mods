<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI name="Invoices">
   <GuiElement type="empty" profile="uiInGameMenuFrame">
        <GuiElement type="empty" profile="ingameMenuFrameHeaderPanel">
            <GuiElement type="bitmap" profile="ingameMenuWeatherHeaderIcon" />
            <GuiElement type="text" profile="ingameMenuFrameHeaderText" text="$l10n_ui_ingameMenuInvoices"/>
        </GuiElement>

        <GuiElement type="empty" profile="ingameMenuSettingsBox" id="mainBox">
            <GuiElement type="empty" profile="ingameMenuSettingsBoxLeft">
                <GuiElement type="text" profile="ingameMenuFrameHeaderText2" text="$l10n_ui_ingameMenuInvoicesIncoming"/>
                <GuiElement type="bitmap" profile="ingameMenuInvoicesHeaderBox">
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="0px 0px" size="200px 40.77px" text="$l10n_ui_ingameMenuInvoicesIncomingDate"/>
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="200px 0px" size="250px 40.77px" text="$l10n_ui_ingameMenuInvoicesIncomingFrom"/>
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="450px 0px" size="150px 40.77px" text="$l10n_ui_ingameMenuInvoicesIncomingAmount"/>
                </GuiElement>
                <GuiElement type="smoothList" profile="ingameMenuInvoicesList" id="invoicesTableIncoming" focusInit="onOpen" listSectionHeader="sectionIncoming" onDoubleClick="onDoubleClickIncoming">
                    <GuiElement type="listItem" profile="ingameMenuInvoicesListItem" name="invoicesTableIncomingRowTemplate" >
                        <GuiElement type="bitmap" profile="ingameMenuInvoicesRowBackground">
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="0px 0px" size="200px 40.77px" name="incomingDate" />
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="200px 0px" size="250px 40.77px" name="incomingFrom" />
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="486px 0px" size="150px 40.77px" name="incomingAmount" />
                        </GuiElement>
                    </GuiElement>
                    <GuiElement type="listItem" profile="ingameMenuContractsListCategory" name="sectionIncoming">
                        <GuiElement type="text" profile="ingameMenuContractsListCategoryTitle" text="Contracts Title" name="title"/>
                    </GuiElement>
                </GuiElement>
                <GuiElement type="threePartBitmap" profile="verticalListSliderRightDocked" size="12px 670px" position="-20px -130px">
                    <GuiElement type="slider" profile="verticalListSliderBar" size="6px 655px" dataElementId="invoicesTableIncoming" id="tableSliderIncoming" handleFocus="false" focusInit="onOpen"/>
                </GuiElement>   
            </GuiElement>

            <GuiElement type="empty" profile="ingameMenuSettingsBoxRight">    
                <GuiElement type="text" profile="ingameMenuFrameHeaderText2" text="$l10n_ui_ingameMenuInvoicesOutgoing"/>
                <GuiElement type="bitmap" profile="ingameMenuInvoicesHeaderBox">
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="0px 0px" size="200px 40.77px" text="$l10n_ui_ingameMenuInvoicesOutgoingDate"/>
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="200px 0px" size="250px 40.77px" text="$l10n_ui_ingameMenuInvoicesOutgoingTo"/>
                    <GuiElement type="text" profile="ingameMenuInvoicesHeaderCell" position="486px 0px" size="150px 40.77px" text="$l10n_ui_ingameMenuInvoicesOutgoingAmount"/>
                </GuiElement>
                <GuiElement type="smoothList" profile="ingameMenuInvoicesList" id="invoicesTableOutgoing" focusInit="onOpen" listSectionHeader="sectionOutgoing" onDoubleClick="onDoubleClickOutgoing">
                    <GuiElement type="listItem" profile="ingameMenuInvoicesListItem" name="invoicesTableOutgoingRowTemplate" >
                        <GuiElement type="bitmap" profile="ingameMenuInvoicesRowBackground">
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="0px 0px" size="200px 40.77px" name="outgoingDate" />
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="200px 0px" size="250px 40.77px" name="outgoingFrom" />
                            <GuiElement type="text" profile="ingameMenuInvoicesRowCell" position="486px 0px" size="150px 40.77px" name="outgoingAmount" />
                        </GuiElement>
                    </GuiElement>
                    <GuiElement type="listItem" profile="ingameMenuContractsListCategory" name="sectionOutgoing">
                        <GuiElement type="text" profile="ingameMenuContractsListCategoryTitle" text="Contracts Title" name="title"/>
                    </GuiElement>
                </GuiElement>
                <GuiElement type="threePartBitmap" profile="verticalListSliderRightDocked" size="12px 670px" position="-20px -130px">
                    <GuiElement type="slider" profile="verticalListSliderBar" size="6px 655px" dataElementId="invoicesTableOutgoing" id="tableSliderOutgoing" handleFocus="false" focusInit="onOpen"/>
                </GuiElement>           
            </GuiElement> 
        </GuiElement>
    </GuiElement>
</GUI>