<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="palletAttachable" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://validation.gdn.giants-software.com/xml/fs22/vehicle.xsd">
    <parentFile xmlFilename="xmlFilename/twinePallets.xml">
        <attributes>
            <set path="vehicle.storeData.image" value="image/store_twine.png"/>
            <set path="vehicle.storeData.brand" value="LIZARD"/>
            <set path="vehicle.storeData.showInStore" value="true"/>
            <set path="vehicle.configurationSets.configurationSet(0)#isDefault" value="true"/>
         </attributes>
    </parentFile>
    <storeData>
        <name>$l10n_name_twine</name>
        <image>image/store_twine.png</image>
        <brand>LIZARD</brand>
        <category>pallets</category>
    </storeData>
</vehicle>
