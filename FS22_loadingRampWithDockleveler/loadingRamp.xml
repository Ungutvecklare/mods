            <part node="sideProtectorPart02">
                <keyFrame time="0"   rotation="0 0 0"/>
                <keyFrame time="0.125"    rotation="7 0 0"/>
                <keyFrame time="0.875"    rotation="7 0 0"/>
                <keyFrame time="1"   rotation="0 0 0"/>				
            </part>			
			
            <part node="docklevelerPart01">
                <keyFrame time="0"    	 rotation="0 0 0"/>
                <keyFrame time="0.25"    rotation="-15 0 0"/>
