                <moving linkNode="docklevelerPart01" template="vectorLoop" />
                <posEnd linkNode="docklevelerPart01" template="clackVar1" />
                <negEnd linkNode="docklevelerPart01" template="clackVar4" />
                <moving linkNode="docklevelerPart02" template="vectorLoop" />
                <posEnd linkNode="docklevelerPart02" template="clackVar4" />
                <negEnd linkNode="docklevelerPart02" template="clackVar6" />				
