            </part>
			
            <part node="docklevelerPart02Colli">
                <keyFrame time="0"    	 rotation="90 0 0"/>
                <keyFrame time="0.30"    rotation="90 0 0"/>
                <keyFrame time="0.70"    rotation="7 0 0"/>
                <keyFrame time="1"    	 rotation="7 0 0"/>				
            </part>
			
            <part node="hydraulicCylinder01Part01">
                <keyFrame time="0"    	 rotation="-33.75 0 0"/>
                <keyFrame time="0.25"    rotation="-60 0 0"/>
                <keyFrame time="0.75"    rotation="-60 0 0"/>
                <keyFrame time="1"    	 rotation="-33.75 0 0"/>				
            </part>
			
            <part node="hydraulicCylinder01Part02">
                <keyFrame time="0"    	 translation="0 0 0.714"/>
                <keyFrame time="0.25"    translation="0 0 1.079"/>
                <keyFrame time="0.75"    translation="0 0 1.079"/>
                <keyFrame time="1"    	 translation="0 0 0.714"/>				
            </part>
			
            <part node="hydraulicCylinder02Part01">
                <keyFrame time="0"    	 rotation="2.95 0 0"/>
